# Git
alias gp='git push'
alias ga='git add'
alias gcam='git commit -m'
alias gst='git status'
alias gco='git checkout'
alias gcb='git checkout -b'
alias gcm='git checkout main'

# General
alias ll='ls -lah'
alias cl='clear'

# Directories
alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

# Opening Programs
alias n="nvim"
alias c="code ."

# Going Home
alias h="cd ~"
